/* Collapse oversized image and pulled images after iPad breakpoint. */
@media (max-width: 767px) {
	#profile_search .bell{ margin-right:30px;}
	#profile_search .input-group{ width:93%;}
	#profile_search h1{ margin:30px 10px; font-size:16px;}
	#header .navbar-default .navbar-nav>li>a, #header .navbar-default .navbar-nav li:last-child a{ text-align:left; padding:10px; }
	.navbar-default .navbar-collapse, .navbar-default .navbar-form
	#header nav .navbar-default .navbar-collapse, #header nav .navbar-default .navbar-form{ background:#f8f8f8;}
	#profile_search .pro_pic span{ margin-left:10px;}
	#header .navbar-toggle{ margin-right:0px;}
	
	
}

@media (max-width: 643px) {
}

@media (max-width: 480px) {
	#profile_search .pro_pic span{ margin-left:0px}
	#profile_search .pro_pic span .text-center{ text-align:left;}
	#profile_search h1{ margin:10px;}
	#profile_search .bell{ margin-right: 22px;
    margin-top: 34px; font-size:16px;}
	#profile_search .pro_pic img{ display:none;}
	#header{ background:#3ca7f9;}
	#header .navbar-toggle{ border:none;}
	#header .navbar-default .navbar-toggle .icon-bar{ background:#fff;}
}

